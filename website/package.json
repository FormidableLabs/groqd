    "@types/node": "^18.7.5",
    "formideploy": "^0.4.2",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "nanoid": "^3.3.6",
    "postcss": "^8.4.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
