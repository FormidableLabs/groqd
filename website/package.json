    "@headlessui/react": "^1.7.14",
    "@mdx-js/react": "^1.6.22",
    "@types/lodash.has": "^4.5.7",
    "@uiw/react-split": "^5.8.10",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "glob": "^10.0.0",
    "groq-js": "^1.1.1",
    "groqd": "workspace:*",
    "js-beautify": "^1.14.7",
    "lodash.debounce": "^4.0.8",
    "lodash.has": "^4.5.2",
    "lz-string": "^1.5.0",
    "monaco-editor": "^0.37.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.8.0",
    "tailwindcss": "^3.3.2",
    "zod": "^3.21.4"
