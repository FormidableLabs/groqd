  "devDependencies": {
    "@types/react": "^18.0.35",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@sanity/icons": "^2.3.1",
    "@sanity/ui": "^1.3.2",
    "react": "^18.2.0",
